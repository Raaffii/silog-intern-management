<div class="row">
  <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Detail (ADMIN)</h4>
        <% if(msg.length !==0 ){ %>
        <div class="alert alert-danger mt-3" role="alert">
          <%= msg %>
        </div>
        <%  }%>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>No</th>
              <th>Nama 1</th>
              <th>Instansi 1</th>
              <th>Nama 2</th>
              <th>Instansi 2</th>
              <th>Nama 3</th>
              <th>Instansi 3</th>
              <th>Detail</th>


            </tr>
          </thead>
          <tbody>
            <% if(data.length===0){ %>
            <tr>
              <td colspan="8" style="text-align: center;">Belum Ada Ajuan Masuk</td>
            </tr>

            <%} %>
            <% data.forEach((ajuan,i) => { %>
            <tr>
              <td><%= ++i %></td>
              <td><%= ajuan.nama1 %></td>
              <td><%= ajuan.instansi1 %></td>
              <td><%= ajuan.nama2 %></td>
              <td><%= ajuan.instansi2 %></td>
              <td><%= ajuan.nama3 %></td>
              <td><%= ajuan.instansi3 %></td>
              <td>
                <a type="submit" href="/admin/dashboard/<%= ajuan.id %>" class="btn-sm btn-dark btn-icon-text">
                  View Detail
                  <i class="mdi mdi mdi-eye btn-icon-append"></i>
                </a>
              </td>
            </tr>

            <% }); %>
          </tbody>
        </table>
      </div>

    </div>
  </div>
</div>
</div>